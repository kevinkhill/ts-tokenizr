{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "target": "es2019",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2019"
    ],
    "keyofStringsOnly": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "exclude": [
    "node_modules"
  ]
}
